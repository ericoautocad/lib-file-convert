{
    "all": true,
    "include": [
        "src/**/*.js"
    ],
    "report-dir": "src/coverage",
    "exclude": [
        "src/index.js",
        "**/*.spec.js",
        "src/coverage"
    ],
    "reporter": [
        "cobertura",
        "html",
        "lcov",
        "text-summary"
    ],
    "check-coverage": true,
    "branches": 75,
    "lines": 85,
    "functions": 85,
    "statements": 85
}